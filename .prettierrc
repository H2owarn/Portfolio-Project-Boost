{
	"useTabs": true,
	"tabWidth": 2,
	"trailingComma": "none",
	"semi": true,
	"singleQuote": true,
	"printWidth": 100,
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": ["<THIRD_PARTY_MODULES>", "\n", "^@/", "^[./]", "<TYPES>"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
